def async produce(channel : chan<int>) {
	while true {
		delay(random(100, 1000));
		random(1, 10) -> channel;
	}
}

def async consume(channel : chan<int>) {

	i : int ;

	while true {
		i <- channel;
		print(i);
	}
	
}

def main() : void {

	channel := chan<int>();

	spawn produce(channel);
	spawn consume(channel);

	return 1;

}